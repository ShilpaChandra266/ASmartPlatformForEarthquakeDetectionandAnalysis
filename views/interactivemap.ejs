                constrainSelection:false,
                fillGrid:true,
                rangeLow:"19700101",
                rangeHigh:"20250101"
            });
        </script>
        <div>
            <input id="end_checkbox"
                   name="end_checkbox"
                   type="checkbox"
                   value="newest"
                   checked="checked"
                   onclick="toggleCheckBoxes();"/>
